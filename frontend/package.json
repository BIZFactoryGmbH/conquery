    "chance": "^1.1.8",
    "compression": "^1.7.4",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "express-rate-limit": "^6.7.0",
    "file-saver": "^2.0.5",
    "helmet": "^5.0.2",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "keycloak-js": "^19.0.1",
    "lodash.difference": "^4.5.0",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.20",
    "react": "^18.1.0",
    "react-dnd": "^16.0.1",
