{
  "name": "conquery",
  "type": "module",
  "version": "0.0.0-SNAPSHOT",
  "description": "conquery",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vite-jest --no-cache",
    "format": "prettier --eslint-config-path ./.eslintrc.js --write '**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "checkformat": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "server": "nodemon -e ts,json,csv,text --watch mock-api --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm mock-api/server\"",
    "serve": "node --experimental-specifier-resolution=node --loader ts-node/esm server",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
