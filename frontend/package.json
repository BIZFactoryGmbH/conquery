  "resolutions": {
    "fsevents": "2.3.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "repository": "github:ingef/conquery",
  "keywords": [
    "conquery",
    "visual",
    "interactive",
    "queries",
    "big",
    "databases"
  ],
  "prettier": {
    "trailingComma": "all",
    "semi": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "proseWrap": "always",
    "importOrder": [
      "^\\.\\./",
      "^\\./"
    ],
    "importOrderSeparation": true,
    "overrides": [
      {
        "files": "*.(ts|tsx)",
        "options": {
          "parser": "typescript"
        }
      }
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
